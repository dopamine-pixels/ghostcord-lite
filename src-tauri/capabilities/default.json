{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "remote": {
    "urls": [
      "https://discord.com/*",
      "https://*.discord.com/*",
      "https://discordapp.com/*",
      "https://*.discordapp.com/*"
    ]
  },
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default"
  ]
}
